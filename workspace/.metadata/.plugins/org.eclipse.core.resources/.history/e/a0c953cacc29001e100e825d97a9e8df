<article id="fetchContent1" name="f"></article>

<script>
window.onload = FetchContent1;
    function FetchContent1() {
        var fetchContent = document.getElementById('fetchContent1');
        for(var i = 0; i < 14; i++){
    		var f = document.getElementsByName("f")[i];
    		if(f.innerHTML != ''){
    			f.innerHTML = '';
    		}
    	}
        if (fetchContent.innerHTML === '') {
            // Fetch 내용을 열기
            fetch('test/ruddud').then(function(response){
	response.text().then(function(text){
		fetchContent.innerHTML = text;
	});
});
        } else {
            // Fetch 내용을 닫기
            fetchContent.innerHTML = '';
        }
    }
</script>

