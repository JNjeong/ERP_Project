package com.lab.erp.dao.a.a1;

import org.apache.ibatis.session.SqlSession;
import org.springframework.beans.factory.annotation.Autowired;

import com.lab.erp.vo.a.a1.Erp_BusinesstypeVO;


public class Erp_BusinesstypeDAO {
private SqlSession sqlSession;
	
	@Autowired
	public Erp_BusinesstypeDAO(SqlSession sqlSession) {
		this.sqlSession = sqlSession;
	}

	
	public int createBusinesstype(Erp_BusinesstypeVO vo) {
		return sqlSession.insert("admin.createAdmin", vo);
	}
	
	public int updateBusinesstype(Erp_BusinesstypeVO vo) {
		return sqlSession.update("admin.updateAdmin", vo);
	}
	
	public int deleteBusinesstype(int businesstype_no) {
		return sqlSession.delete("admin.deleteAdmin", businesstype_no);
	}
	
	
}
